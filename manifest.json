{
  "name": "Jiftt",
  "description": "Jira fst time tracking",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries"]},
    "storage",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "assets/timer-16.png",
    "32": "assets/timer-32.png",
    "64": "assets/timer-64.png",
    "128": "assets/timer-128.png"
  }
}
